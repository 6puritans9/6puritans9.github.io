		"eslint-plugin-storybook": "^10.2.10",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.3.0",
		"playwright": "^1.58.1",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"storybook": "^10.2.10",
		"svelte": "^5.51.0",
		"svelte-check": "^4.3.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
